/* トップバー */
.contain-to-grid, .menu, .top-bar-section li:not(.has-form) a:not(.button), .top-bar.expanded .title-area {
    background-color: transparent;
z-index: 1;
}
.title-area img {
    width: 80px;
    height: auto;
    position: relative;
    top: 0;
    left: 10px;
}
.top-bar-section li:not(.has-form) a:not(.button):hover {
    background-color: transparent;
    color: #91B31B;
}
.top-bar-section .f-dropdown li {
    width: 200px; 
}
.top-bar-section ul{
background-color: transparent;
}
.top-bar-section ul .f-dropdown {
  background-color: rgba(255,255,255,0.9);
}
.top-bar-section ul li>a{
color: #555;
}
.menu.top-bar {
    background-color: rgba(255,255,255,0.6);
box-shadow: 0px 1px 5px rgba(0, 0, 0, 0.15);
}
.top-bar-section .has-dropdown>a:after {
  content: "";
  display: block;
  width: 0;
  height: 0;
  border: inset 5px;
  border-color: rgba(0,0,0,0.4) transparent transparent transparent;
  border-top-style: solid;
  margin-top: -2.5px;
  top: 22.5px;
}
ul#drop-news li a {
    line-height: 2.5;
}
.top-bar-section li {
  position: relative;
}
.top-bar-section .fa.fa-tint {
  position: absolute;
  bottom: 15px;
  left: -6px;
  font-size: 12px;
color: #89d0d5;
}
.top-bar-section ul li:hover>a {
  color: inherit;
}
.toggle-button.hide-for-large-up{
background-color: #b9d98b;
}

/* ヘッダー */
.header {
    padding: 20px 0 0px 0;
    text-align: center;
}
.header h1 {
    margin: 0;
    font-size: 26px;
    line-height: 1;
}
.header h1 a {
    color: #000;
    text-decoration: none;
}
.header p {
    margin: 8px 0 0 0;
    font-size: 12px;
    line-height: 1;
}
.header .logo {
    margin: 0 0 10px 0;
    border: none;
    vertical-align: bottom;
}
.top-bar .name h1 a {
color:  #333;
}
.top-bar .name h1 a:hover {
color: #91B31B;
}
.hide-for-large-up a:hover,a:focus {
    color: #91B31B;
}
.top-bar .row {
    max-width: 1260px;
}
@media only screen and (min-width: 40.063em){
.top-bar-section .has-dropdown>a:after {
  border-color: rgba(0,0,0,0.4) transparent transparent transparent;
  right: 6px;
}
}
.breadcrumbs>*:hover a, .breadcrumbs>*:focus a {
  text-decoration: none;
}
.breadcrumbs a:hover span, .breadcrumbs a:hover i {
    color: #91B31B;
}
.f-dropdown li:hover, .f-dropdown li:focus {
    color: #91B31B;
    background: transparent;
 }
.f-dropdown li a:hover{
    color: #91B31B;
}
/*トップメインイメージ部分 */
.slide {
    height: 280px;
    padding: 50px 20px 20px 20px;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    background-color: #dfe3e8;
    background-image: url(<$mt:BlogURL encode_html="1"$>images/slide01.jpg);
    background-position: 50% 0%;
    background-size: cover;
    margin: 20px 0 0 0;
}
.slide h3 {
    margin: 0;
    font-size: 26px;
    line-height: 1;
}
.slide h3 a {
    color: #000;
    text-decoration: none;
}

.slide p {
    margin: 8px 0 0 0;
    font-size: 14px;
    line-height: 1.5;
}

/* 枠の設定 */
.slide .inner {
    padding: 20px;
    background-color: rgba(255,255,255,0.6);
}

/* パーツを重ねる設定 */
.slide {
    position: relative;
}
.slide .inner {
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
}
.orbit-container {
    margin-bottom: 10px
}
#mainvisual {
    margin: 20px 0 25px;
}


/* 下層ページバナー */
.page-banner {
    background-color: #dfe3e8;
    background-image: url("<$mt:BlogURL encode_html="1"$>images/slide01.jpg");
    background-position: 50% 0;
    background-size: cover;
    box-sizing: border-box;
    height: 140px;
}
.page-banner h2 {
    font-size: 2.2em;
}
.page-banner .inner {
    background-color: rgba(255, 255, 255, 0.7);
    padding: 44px;
    height: 140px;
}

/* 下層ページ */
.breadcrumbs {
    border: none;
    border-radius: 0;
    background-color: transparent;
    padding: 0;
}
ul.breadcrumbs {
    margin-bottom: 0;
}
.breadcrumbs > * a, .breadcrumbs > * span {
    color: #333333;
    font-size: 0.75rem;
    text-transform: none;
}
.entry-meta {
    margin-bottom: 10px;
}
.page-title {
    font-size: 1.5rem; 
}
article {
  background-color: #fff;
padding: 20px;
  margin-bottom: 40px;
line-height: 1.6;
    box-shadow: 0 0 4px rgba(0,0,0,.3);
}

/* ページ送り */
.page-navigation {
    text-align: center;
}
.page-navigation ul {
    display: block;
    padding-bottom: 10px;
}
.page-navigation li {
    display: inline-block;
    border-bottom: none;
    list-style: none;
}
.page-navigation li a {
    padding: 0 10px;
}
.page-navigation li a[rel="prev"]::before {
    content: "«";
    margin-right: 0.3em;
}

.page-navigation li a[rel="next"]::after {
    content: "»";
    margin-left: 0.3em;
}

