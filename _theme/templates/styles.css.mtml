}
.entry{
padding: 25px 25px 45px;
}
#content h2{
font-size: 1.3rem;
}
.page-banner h2 {
    font-size: 1.2em;
}
    .news time{
        float: none;
    }
    .icon-new{
        margin-left: 0;
    }
    .news-title{
        margin-left: 0;
        padding-left: 0;
    }
    .more-button {
        background-color: #BAD98C;
        background-image: none;
       color: #fff;
        padding: 10px;
        width: 100%;
        text-align: center;
        right: 0;
    }
   .more-button:hover, .more-button:focus {
