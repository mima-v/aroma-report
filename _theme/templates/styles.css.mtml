.bx-wrapper .bx-prev {
	background: url(/images/control-l.png) no-repeat;
background-size:  16px 27px;
left: 10px;
}

.bx-wrapper .bx-next {
	background: url(/images/control-r.png) no-repeat;
background-size:  16px 27px;
right: 10px;
}
.bx-wrapper .bx-controls-direction a {
	position: absolute;
	top: 40%;
	outline: 0;
	width: 16px;
	height: 27px;
	text-indent: -9999px;
	z-index: 9999;
}

