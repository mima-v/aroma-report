.blog-new{
background-color: #fff;
border: solid 2px #BAD98C;
margin-top: 10px;
}
.blog-new a {
    padding: 10px 6px;
    border-bottom: 1px dashed #BAD98C;
    margin: 0;
    display: block;
line-height: 1.4;
}
.blog-new a:last-child {
    border: none;
}
.blog-new a > div {
    font-size: 0.8rem;
    margin-bottom: 2px;
}
.blog-new a:hover h2 {
color: #91B31C;
}
.blog-new h2 i{
margin-right: 6px;
}
.blog-title{
background-color: #BAD98C;
    color: #fff;
    padding: 7px;
}
#entry-body a{
text-decoration: underline;
}
.archive-list{
background-color: #fff;
border: solid 2px #BAD98C;
}
.category-list{
background-color: #fff;
border: solid 2px #BAD98C;
margin-top: 10px;
}
.category-list li {
    padding: 6px;
    border-bottom: 1px dashed #BAD98C;
    margin: 0;
    display: block;
    font-size: 0.9em;
}
.category-list li:last-child {
    border: none;
}
.category-list h2{
    margin-bottom: 0;
}
.category-list-title{
background-color: #BAD98C;
    color: #fff;
    padding: 4px 0 4px 7px;
}
.archive-list li {
    padding: 6px;
    border-bottom: 1px dashed #BAD98C;
    margin: 0;
    display: block;
    font-size: 0.9em;
}
.archive-list li:last-child {
    border: none;
}
.archive-list .archive-list-title{
    background-color: #BAD98C;
    color: #fff;
    padding: 7px;
margin-bottom: 0;
}
.btn-contact img{
width: 170px;
height: auto
}
.copyright{
text-align: center;
margin: 10px 0 0 0;
}
/* min-width 1025px */
@media only screen and (max-width: 64.063em) { 
   ul.sns {
       padding: 0 0 5px;
       background-color: transparent;
       position: relative;
       top: 0;
       left: 0;
   }
   ul.sns li{
      display: inline-block;
       padding: 0;
       width: 55px;
   }
   .pocket .fa-get-pocket {
       font-size: 1.8em;
   }
}
/* 640px以下のスマホ表示 */
@media only screen and (max-width: 40em) { 
.page-banner{
    height: 80px;
}
.page-banner .inner{
    padding: 30px;
    height: 80px;
}
.blog-new a > div {
    font-size: 0.9em;
}
.news li a{
font-size: 1.1em;
}
.entry div.entry-meta {
